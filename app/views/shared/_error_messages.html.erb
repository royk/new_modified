<% if object.errors.any? %>
<div id="error_explanation">
	<div class="alert alert-error">The form contains <%= pluralize(object.errors.count, "error") %></div>
	<ul>
		<% object.errors.full_messages.each do |msg| %>
			<% if !msg.index("digest") %>
				<li>* <%= msg %></li>
			<% end %>
		<% end %>
	</ul>
</div>
<% end %>