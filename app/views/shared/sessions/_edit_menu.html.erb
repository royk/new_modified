<% if signed_in? && !training_session.nil? && (current_user.admin? || current_user==training_session.user) %>
	<li class="dropdown" style="float:left;">
		<a href="#" class="dropdown-toggle" data-toggle="dropdown">
			<i class="icon-cog"></i>
		</a>
		<ul class="dropdown-menu">
			<li><%= link_to "Edit Session", edit_training_session_path(training_session) %></li>
			<li><%= link_to t(:delete), training_session, method: :delete, data: {confirm: t(:delete_confirmation)} %></li>
		</ul>
	</li>
<% end %>